{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "242 цпп",
  "mainBinaryName": "mtc-app",
  "version": "0.1.0",
  "identifier": "com.mtc-cms.app",
  "build": {
    "frontendDist": "https://242.mil.gov.ua",
    "devUrl": "https://242.mil.gov.ua",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "plugins": {
    "shell": {
      "open": "**"
    }
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "allowDowngrades": true,
      "certificateThumbprint": null,
      "digestAlgorithm": null,
      "nsis": null,
      "signCommand": null,
      "timestampUrl": null,
      "tsp": false,
      "webviewInstallMode": {
        "silent": true,
        "type": "downloadBootstrapper"
      },
      "wix": {
        "language": "uk-UA"
      }
    }
  }
}
